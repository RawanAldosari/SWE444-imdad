    actions: <Widget>[
          Padding(
            padding: EdgeInsets.only(right: 15.0),
            child: GestureDetector(
              onTap: () {
                Navigator.pushReplacement(context,
                    MaterialPageRoute(builder: (context) => SignupLoginScreen()));
              },
              child: Icon(Icons.arrow_forward_ios),
            ),
          )
        ],
